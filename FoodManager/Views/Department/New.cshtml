@model FoodManager.Models.Department
@using FoodManager.Infrastructure.Enums
@using FoodManager.Infrastructure.Helpers
@{
    ViewBag.Title = "Nuevo departamento";
    ViewBag.ButtonSave = Html.ButtonSave();
}

@using (Html.BeginForm(ActionType.Create.ToString(),  EntityType.Department.ToString(), FormMethod.Post, new {id = "Form" }))
{
    <div class="panel panel-default">
        <div class="panel-heading"> Información general</div>
        <div class="panel-body">
            <div id="DepertamentContent">
                @Html.Partial("_Form", Model)
            </div>
        </div>
    </div>
    @Html.ButtonsForm()
}